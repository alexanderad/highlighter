{
  "manifest_version": 2,
  "name": "Highlighter",
  "description": "Sends the page to Highlighter.",
  "version": "0.1",
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_title": "Click to send this page to Highlighter!"
  },
  "background": {
    "scripts": ["background.js"]
  }
}
